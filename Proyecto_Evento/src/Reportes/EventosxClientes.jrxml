	<queryString>
		<![CDATA[select  
c.nombre as Nombre_Cliente,
c.apellido as Apellido_Cliente,
e.nombre as Nombre_evento,
e.fecha_evento,
s.nombre as Nombre_salon
from evento e
inner join cliente c on c.id_cliente=e.id_cliente
inner join salon s on s.id_salon=e.id_salon
group by c.nombre,
c.apellido,
e.nombre,
e.fecha_evento,
s.nombre;]]>
