	<queryString>
		<![CDATA[select
e.nombre as Nombre_Evento,
s.nombre as Nombre_Servicio,
es.precio 
from evento e
inner join evento_servicio es
on es.id_evento=e.id_evento
inner join servicio s on 
s.id_servicio=es.id_servicio
group by 
e.nombre ,
s.nombre,
es.precio ]]>
