	<parameter name="id_venta" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
e.nombre,
sum(v.cantidad_boletos*e.precio_boleto) as monto
from venta_boleto v 
inner join evento e 
on e.id_evento = v.id_evento
where v.id_venta_boleto = $P{id_venta}
group by e.nombre]]>
	</queryString>
